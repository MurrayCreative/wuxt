{
  "name": "wuxt",
  "version": "1.0.0",
  "description": "Initial wuxt app",
  "author": "Northosts AB",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon server/index.js --watch server",
    "wp-json": "json-server --watch tools/wp-rest-api/data/db.json --config tools/wp-rest-api/json-server.json --routes tools/wp-rest-api/routes.json --middlewares tools/wp-rest-api/middleware/singular.js",
    "build": "nuxt build",
    "start": "cross-env NODE_ENV=production node server/index.js",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "test:unit": "cross-env TEST=unit nyc ava --config ava.config.unit.js",
    "test:e2e": "cross-env TEST=e2e nyc ava --config ava.config.e2e.js",
    "test:update": "ava --update-snapshots",
    "test": "npm run test:unit & npm run test:e2e",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.3.6",
    "@nuxtjs/pwa": "^2.6.0",
    "@nuxtjs/style-resources": "^0.1.2",
    "cross-env": "^5.2.0",
    "element-ui": "2.5.4",
    "express": "^4.16.4",
    "google-maps": "3.3.0",
    "nouislider": "13.1.0",
    "nuxt": "2.8.0",
    "nuxt-webfontloader": "^1.1.0",
    "remove": "^0.1.5",
    "v-tooltip": "2.0.0-rc.33",
    "vue-clickaway": "2.2.2",
    "vue-github-buttons": "3.0.1",
    "vue-lazyload": "1.2.6",
    "vue-material": "^1.0.0-beta-11",
    "vue-product-zoomer": "2.0.11",
    "vue2-transitions": "0.2.3",
    "wpapi": "^1.2.1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.4.4",
    "@babel/register": "^7.4.4",
    "@nuxtjs/eslint-config": "^0.0.1",
    "@vue/babel-preset-app": "^3.9.2",
    "@vue/server-test-utils": "^1.0.0-beta.29",
    "@vue/test-utils": "^1.0.0-beta.29",
    "ava": "^2.2.0",
    "babel-eslint": "^10.0.1",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-register": "^6.26.0",
    "browser": "^0.2.6",
    "browser-env": "^3.2.6",
    "coffee-loader": "^0.9.0",
    "coffeescript": "^2.4.1",
    "eslint": "^5.15.1",
    "eslint-config-prettier": "^4.1.0",
    "eslint-config-standard": ">=12.0.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": ">=2.16.0",
    "eslint-plugin-jest": ">=22.3.0",
    "eslint-plugin-node": ">=8.0.1",
    "eslint-plugin-nuxt": ">=0.4.2",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-promise": ">=4.0.1",
    "eslint-plugin-standard": ">=4.0.0",
    "eslint-plugin-vue": "^5.2.2",
    "jsdom-global": "^3.0.2",
    "json-server": "^0.15.0",
    "node-sass": "^4.12.0",
    "nodemon": "^1.19.0",
    "nyc": "^14.1.1",
    "prettier": "^1.16.4",
    "pug": "2.0.3",
    "pug-plain-loader": "^1.0.0",
    "require-extension-hooks": "^0.3.3",
    "require-extension-hooks-babel": "^1.0.0",
    "require-extension-hooks-vue": "^3.0.0",
    "sass-loader": "^7.1.0"
  },
  "jshintConfig": {
    "esversion": 6,
    "asi": true
  }
}
